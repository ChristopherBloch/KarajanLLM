{
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token"
    },
    "trustedProxies": ["0.0.0.0/0", "::0/0"]
  },
  "agents": {
    "defaults": {
      "maxConcurrent": 4,
      "model": {
        "primary": "litellm/glm-local",
        "fallbacks": ["litellm/kimi-local"]
      },
      "models": {
        "litellm/glm-local": { "alias": "GLM-4.7 Local" },
        "litellm/kimi-local": { "alias": "Kimi K2 Local" }
      },
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "litellm": {
        "baseUrl": "http://litellm:4000/v1",
        "apiKey": "sk-aria-local-key",
        "api": "openai-responses",
        "models": [
          {
            "id": "glm-local",
            "name": "GLM-4.7-Flash-REAP 23B via LiteLLM",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 202752,
            "maxTokens": 8192
          }
        ]
      }
    }
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  }
}
